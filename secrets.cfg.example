[general]
notifyUsers = ['root']

[google]
domain = yourdomain.com
serviceAccount = dev-console-service-account-user@developer.gserviceaccount.com
serviceAccountKey = dev-console-service-account-password
adminToImpersonate = some-admin@yourdomain.com
blacklistedUsers = ['some-admin@yourdomain.com']

### Set this option to True if using domain aliases ###
replaceDomain = False


[samba]
sambaPrivate = /usr/local/samba/private
sambaPath = DC=YOURDOMAIN,DC=COM
adBase = ou=Domain Users,dc=yourdomain,dc=com
blacklistedUsers = ['Guest', 'Administrator', 'krbtgt', 'some-other-user']